    <property name="searchPosition" display-name-key="Search Position" description-key="Search Position" of-type="Enum" usage="bound" required="true" default-value="top-right">
      <value name="bottom-leading" display-name-key="Bottom Leading">bottom-leading</value>
      <value name="bottom-left" display-name-key="Bottom Left">bottom-left</value>
      <value name="bottom-right" display-name-key="Bottom Right">bottom-right</value>
      <value name="bottom-trailing" display-name-key="Bottom Trailing">bottom-trailing</value>
      <value name="top-leading" display-name-key="Top Leading">top-leading</value>
      <value name="top-left" display-name-key="Top Left">top-left</value>
      <value name="top-right" display-name-key="Top Right">top-right</value>
      <value name="top-trailing" display-name-key="Top Trailing">top-trailing</value>
    </property>
    <property name="legendEnabled" display-name-key="Legend Enabled" description-key="Legend Enabled" of-type="TwoOptions" usage="bound" required="true" default-value="true" />
    <property name="legendPosition" display-name-key="Legend Position" description-key="Legend Position" of-type="Enum" usage="bound" required="true" default-value="bottom-left">
      <value name="bottom-leading" display-name-key="Bottom Leading">bottom-leading</value>
      <value name="bottom-left" display-name-key="Bottom Left">bottom-left</value>
      <value name="bottom-right" display-name-key="Bottom Right">bottom-right</value>
      <value name="bottom-trailing" display-name-key="Bottom Trailing">bottom-trailing</value>
      <value name="top-leading" display-name-key="Top Leading">top-leading</value>
      <value name="top-left" display-name-key="Top Left">top-left</value>
      <value name="top-right" display-name-key="Top Right">top-right</value>
      <value name="top-trailing" display-name-key="Top Trailing">top-trailing</value>
    </property>
    <resources>
      <code path="index.ts" order="1" />
